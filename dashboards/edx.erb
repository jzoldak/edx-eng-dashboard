<script type='text/javascript'>
$(function() {
    Dashing.gridsterLayout('[{"col":9,"row":1},{"col":3,"row":4},{"col":1,"row":4},{"col":9,"row":4},{"col":3,"row":6},{"col":1,"row":6},{"col":5,"row":6},{"col":5,"row":4},{"col":5,"row":1},{"col":3,"row":1},{"col":1,"row":1},{"col":7,"row":1}]')
